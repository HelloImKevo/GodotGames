config_version=5

[application]

config/name="BulletRealm"
config/description="An online multiplayer \"Bullet Hell\" game."
config/version="0.0.1"
run/main_scene="res://main_login/main_login.tscn"
config/features=PackedStringArray("4.2", "Mobile")
